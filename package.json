{
  "name": "alysia-backend",
  "version": "0.0.1",
  "description": "alysia Backend",
  "author": "Shahjalal",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && tsc-alias",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db": "ts-node ./scripts/database/index.ts",
    "db:mi:generate": "ts-node ./scripts/database/index.ts migration:generate",
    "db:mi:run": "ts-node ./scripts/database/index.ts migration:run",
    "db:mi:revert": "ts-node ./scripts/database/index.ts migration:revert",
    "db:mi:show": "ts-node ./scripts/database/index.ts migration:show",
    "db:seed": "ts-node ./scripts/database/index.ts seed:run",
    "db:seed:create": "ts-node ./scripts/database/index.ts seed:create",
    "db:info": "ts-node ./scripts/database/index.ts info",
    "db:backup": "ts-node ./scripts/database/index.ts backup",
    "db:restore": "ts-node ./scripts/database/index.ts restore",
    "db:clean": "ts-node ./scripts/database/index.ts clean",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "nodemailer": "^6.9.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "twilio": "^4.20.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.7.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
